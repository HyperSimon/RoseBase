<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingItemView">

        <!--是否有可选框-->
        <attr name="item_checkbox" format="boolean"></attr>

        <!--当前选项的类型 开头，中间，和底部-->
        <attr name="item_position">
            <enum name="top" value="1"></enum>
            <enum name="middle" value="2"></enum>
            <enum name="bottom" value="3"></enum>
            <enum name="single" value="4"></enum>
        </attr>

        <attr name="item_content" format="string"></attr>

    </declare-styleable>
</resources>
