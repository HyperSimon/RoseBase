<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:enterFadeDuration="200"
    android:exitFadeDuration="200" tools:keep="@drawable/selector_dialog_btn_cancle">

    <!--按下-->
    <item android:state_pressed="true">
        <shape>
            <stroke
                android:width="1dp"
                android:color="@color/white_10Alpha"
                android:dashGap="0dp">
            </stroke>
            <solid android:color="@color/orange"/>
        </shape>
    </item>

    <!--正常情况-->
    <item>
        <shape>
            <stroke
                android:width="1dp"
                android:color="@color/white"
                android:dashGap="0dp">
            </stroke>
            <solid android:color="@color/sun_flower"/>
        </shape>
    </item>
</selector>